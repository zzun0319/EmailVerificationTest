package com.junhee.email.model;

import java.util.List;

public class Item {
	
	private Long id;
	private String itemName;
	private UploadFile attachFile;
	private List<UploadFile> imageFiles;
	
	public Item() {
		// TODO Auto-generated constructor stub
	}

	public Item(Long id, String itemName, UploadFile attachFile, List<UploadFile> imageFiles) {
		this.id = id;
		this.itemName = itemName;
		this.attachFile = attachFile;
		this.imageFiles = imageFiles;
	}

	public Long getId() {
		return id;
	}

	public void setId(Long id) {
		this.id = id;
	}

	public String getItemName() {
		return itemName;
	}

	public void setItemName(String itemName) {
		this.itemName = itemName;
	}

	public UploadFile getAttachFile() {
		return attachFile;
	}

	public void setAttachFile(UploadFile attachFile) {
		this.attachFile = attachFile;
	}

	public List<UploadFile> getImageFiles() {
		return imageFiles;
	}

	public void setImageFiles(List<UploadFile> imageFiles) {
		this.imageFiles = imageFiles;
	}

	@Override
	public String toString() {
		return "Item [id=" + id + ", itemName=" + itemName + ", attachFile=" + attachFile + ", imageFiles=" + imageFiles
				+ "]";
	}
	
}
